{
  "type": "object",
  "properties": {
    "cabinetId": {
      "type": "string",
      "description": "ID кабинета, для которого создается поставка.\n    Используется для привязки поставки к конкретному кабинету и проверки прав доступа.",
      "example": "123e4567-e89b-12d3-a456-426614174000"
    },
    "deliveryDate": {
      "type": "string",
      "description": "Дата поставки.\n    Указывается в формате ISO 8601.\n    Используется для планирования и отслеживания поставок.",
      "example": "2024-02-26T12:00:00Z"
    },
    "cargoPlaces": {
      "type": "number",
      "description": "Количество грузовых мест.\n    Необязательное поле.\n    Используется для планирования логистики.",
      "example": 10
    },
    "products": {
      "description": "Массив товаров в поставке.\n    Должен содержать хотя бы один товар.\n    Каждый товар должен иметь:\n    - ID товара в WB\n    - Количество (больше нуля)\n    - Цену (больше нуля)\n    - Выбранные услуги (опционально)\n    - Выбранные расходники (опционально)\n    - ID поставщика",
      "minItems": 1,
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "wbProductId": {
            "type": "string",
            "description": "ID товара в WB",
            "example": "WB123456"
          },
          "quantity": {
            "type": "number",
            "description": "Количество товара",
            "example": 100,
            "minimum": 1
          },
          "price": {
            "type": "number",
            "description": "Цена товара",
            "example": 50,
            "minimum": 0.01
          },
          "selectedServices": {
            "description": "Выбранные услуги",
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "description": "ID услуги",
                  "example": "123e4567-e89b-12d3-a456-426614174000"
                },
                "name": { "type": "string", "description": "Название услуги", "example": "Стирка" },
                "price": {
                  "type": "number",
                  "description": "Цена услуги",
                  "example": 10,
                  "minimum": 0.01
                }
              },
              "required": ["id", "name", "price"],
              "x-readme-ref-name": "ServiceDto"
            }
          },
          "selectedConsumables": {
            "description": "Выбранные расходники",
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "description": "ID расходника",
                  "example": "123e4567-e89b-12d3-a456-426614174000"
                },
                "name": {
                  "type": "string",
                  "description": "Название расходника",
                  "example": "Зип-пакет"
                },
                "price": {
                  "type": "number",
                  "description": "Цена расходника",
                  "example": 5,
                  "minimum": 0.01
                }
              },
              "required": ["id", "name", "price"],
              "x-readme-ref-name": "ConsumableDto"
            }
          },
          "supplierId": {
            "type": "string",
            "description": "ID поставщика",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          }
        },
        "required": [
          "wbProductId",
          "quantity",
          "price",
          "selectedServices",
          "selectedConsumables",
          "supplierId"
        ],
        "x-readme-ref-name": "ProductDto"
      }
    }
  },
  "required": ["cabinetId", "deliveryDate", "products"],
  "x-readme-ref-name": "CreateDeliveryDto"
}
